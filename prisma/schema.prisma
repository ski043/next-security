generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id    String @id @default(uuid())
  email String
  name  String

  createdAt DateTime @default(now())
}

model Blog {
  id String @id @default(uuid())

  content String
}
